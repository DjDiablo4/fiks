package cenzorky;

import java.io.File; 
import java.util.Scanner;

public class Process {
	private String path;
	
	public Process(String path) throws Exception {
	this.path = path;
	readInput = readinput(this.path);
  }
	
	private double readInput(String path) {
		File file = 
			      new File("C:\\Users\\pankaj\\Desktop\\test.txt"); 
			    Scanner sc = new Scanner(file); 
			  
			    while (sc.hasNextLine()) 
			      System.out.println(sc.nextLine()); 
	}
} 
